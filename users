CREATE TABLE APP.ADM_USERS
	(USERNAME VARCHAR(64) NOT NULL, PASSWORD VARCHAR(64) NOT NULL, PRIMARY KEY(USERNAME));

CREATE TABLE APP.ADM_GROUPS
	(USERNAME VARCHAR(64) NOT NULL, GROUPNAME VARCHAR(64) NOT NULL, PRIMARY KEY(USERNAME, GROUPNAME));

